{
  "name": "next-wp",
  "version": "0.0.1",
  "description": "Next.JS package for using headless WordPress as an API",
  "author": "Michael Nelson",
  "license": "ISC",
  "contributors": [
    {
      "name": "Michael Nelson",
      "email": "michael@nelson.tech",
      "url": "https://github.com/nelson-tech"
    }
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "docs": "npm typedoc",
    "build": "tsc && vite build && typedoc",
    "build:watch": "tsc && vite build --watch && typedoc",
    "dev": "vite && typedoc",
    "prepare": "npm run prettier && npm run build",
    "prettier": "prettier --write .",
    "preview": "vite preview",
    "test": "vitest --coverage",
    "test:run": "vitest run --coverage"
  },
  "keywords": [
    "npm",
    "node",
    "next",
    "next.js",
    "wordpress",
    "headless",
    "api",
    "typescript"
  ],
  "dependencies": {
    "luxon": "^3.3.0",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.0",
    "@testing-library/react": "^14.0.0",
    "@types/luxon": "^3.3.0",
    "@types/node": "^20.1.2",
    "@vitest/coverage-c8": "^0.31.0",
    "husky": "^8.0.3",
    "msw": "^1.2.1",
    "prettier": "^2.8.8",
    "rollup-plugin-typescript-paths": "^1.4.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "tslib": "^2.5.0",
    "typedoc": "^0.24.7",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vite-plugin-progress": "^0.0.7",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.31.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nelson-tech/next-wp.git"
  },
  "homepage": "https://github.com/nelson-tech/next-wp#readme",
  "bugs": {
    "url": "https://github.com/nelson-tech/next-wp/issues"
  }
}
